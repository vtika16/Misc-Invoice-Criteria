
IF [Warehouse Graded Date] >= '2016-10-01' 
AND [Warehouse Graded Date] <= '2016-11-02' 
AND ([Warehouse_Grade] = 'A' 
OR [Warehouse_Grade] = 'B' 
OR [Warehouse_Grade] = 'C') 
AND [Credit_Flag] = 0
THEN 'Received - Qualified'

ELSEIF [Warehouse Graded Date] >= '2016-10-01' 
AND [Warehouse Graded Date] <= '2016-11-02' 
AND ([Warehouse_Grade] = 'D' 
OR [Warehouse_Grade] = 'E' 
OR [Warehouse_Grade] = 'BER') 
AND [Credit_Flag] = 0
THEN 'Received - Not Qualified'

ELSEIF [Puerto_Rico_IND] = 0 
AND Contains([Lease_Event], 'Purchase') 
AND [SPT_PPO_Percent_PaidAdjusted] >= 1
AND IsEmpty([Warehouse_Receipt_Date]) 
THEN 'PPO'

ELSEIF [Puerto_Rico_IND] = 0 
AND IsEmpty([Warehouse_Receipt_Date]) 
AND Contains([Percent_PPO_Paid_Bucket], '100%+') 
AND (Contains([Lease_Status], 'Closed') 
OR Contains([Lease_Status], 'Cancelled')) 
THEN 'PPO'

ELSEIF [Months_Since_Current_Status] > 1
AND (!Contains([Lease_Event], 'Ownership')
AND !Contains([Lease_Event], 'Collections')
AND !Contains([Lease_Event], 'Purchase')
AND !Contains([Lease_Event], 'Fraud')
AND !Contains([Lease_Event], 'Military')
AND !Contains([Lease_Event], 'Return Confirmed'))
AND IsEmpty([Warehouse_Receipt_Date])
AND isEmpty([WriteOff_Date])
THEN 'Non-Receipt from Customer'

ELSEIF [Months_Since_Current_Status] > 1
AND (!Contains([Lease_Event], 'Ownership')
AND !Contains([Lease_Event], 'Collections')
AND !Contains([Lease_Event], 'Purchase')
AND !Contains([Lease_Event], 'Fraud')
AND !Contains([Lease_Event], 'Military'))
AND Contains([Lease_Event], 'Return Confirm')
AND IsEmpty([Warehouse_Receipt_Date])
AND isEmpty([WriteOff_Date])
THEN 'Non-Receipt from Sprint'

ELSEIF [Months_Since_Current_Status] > 1
AND IsEmpty([Warehouse_Receipt_Date])
AND Contains([Lease_Event], 'Ownership')
AND (Contains([Lease_Status], 'Closed') 
OR Contains([Lease_Status], 'Cancelled'))
THEN 'Change of Ownership'

ELSEIF IsEmpty([Warehouse_Receipt_Date])
AND Contains([Lease_Event], 'Military')
AND [Puerto_Rico_IND] = 0
THEN 'Military'

ELSEIF IsEmpty([Warehouse_Receipt_Date])
AND Contains([Lease_Event], 'Fraud')
AND [Puerto_Rico_IND] = 0
THEN 'Fraud'

ELSE 'NULL'

ENDIF